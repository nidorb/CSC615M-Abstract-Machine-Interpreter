digraph {
	A [label=W fillcolor=green shape=circle style=filled]
	B [label=SR fillcolor=yellow shape=circle style=filled]
	C [label=W fillcolor=yellow shape=circle style=filled]
	D [label=R fillcolor=yellow shape=circle style=filled]
	E [label=SR fillcolor=yellow shape=circle style=filled]
	F [label=R fillcolor=yellow shape=circle style=filled]
	G [label=W fillcolor=yellow shape=circle style=filled]
	H [label=SL fillcolor=yellow shape=circle style=filled]
	I [label=SR fillcolor=yellow shape=circle style=filled]
	J [label=W fillcolor=yellow shape=circle style=filled]
	K [label=SR fillcolor=yellow shape=circle style=filled]
	L [label=R fillcolor=yellow shape=circle style=filled]
	M [label=SR fillcolor=yellow shape=circle style=filled]
	N [label=R fillcolor=yellow shape=circle style=filled]
	A -> B [label="#"]
	B -> C [label=a]
	B -> D [label=b]
	C -> B [label=X]
	D -> E [label=X]
	E -> D [label=b]
	E -> F [label=c]
	E -> F [label="#"]
	F -> G [label="#"]
	G -> H [label="#"]
	H -> H [label=b]
	H -> I [label=a]
	I -> I [label=a]
	I -> J [label=b]
	J -> K [label=X]
	K -> J [label=b]
	K -> L [label=c]
	L -> M [label=X]
	M -> L [label=c]
	M -> N [label="#"]
	N -> accept [label="#"]
}
