digraph {
	A [label=R fillcolor=green shape=circle style=filled]
	B [label=SR fillcolor=yellow shape=circle style=filled]
	C [label=SR fillcolor=yellow shape=circle style=filled]
	A -> B [label="0/X"]
	A -> D [label=Y]
	A -> reject [label=1]
	A -> E [label="1/2"]
	B -> C [label="#"]
	C -> accept [label=1]
}
