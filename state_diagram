digraph {
	A [label=W fillcolor=green shape=circle style=filled]
	B [label=SR fillcolor=yellow shape=circle style=filled]
	C [label=W fillcolor=yellow shape=circle style=filled]
	D [label=W fillcolor=yellow shape=circle style=filled]
	E [label=SR fillcolor=yellow shape=circle style=filled]
	F [label=SR fillcolor=yellow shape=circle style=filled]
	G [label=R fillcolor=yellow shape=circle style=filled]
	H [label=R fillcolor=yellow shape=circle style=filled]
	I [label=R fillcolor=yellow shape=circle style=filled]
	J [label=W fillcolor=yellow shape=circle style=filled]
	K [label=SL fillcolor=yellow shape=circle style=filled]
	L [label=W fillcolor=yellow shape=circle style=filled]
	M [label=W fillcolor=yellow shape=circle style=filled]
	N [label=SL fillcolor=yellow shape=circle style=filled]
	O [label=R fillcolor=yellow shape=circle style=filled]
	P [label=R fillcolor=yellow shape=circle style=filled]
	Q [label=R fillcolor=yellow shape=circle style=filled]
	A -> B [label="#"]
	B -> C [label=0]
	B -> D [label=1]
	B -> E [label=X]
	C -> B [label=Y]
	D -> B [label=Z]
	E -> E [label=1]
	E -> E [label=0]
	E -> F [label=X]
	F -> G [label=0]
	F -> H [label=1]
	F -> I [label="#"]
	G -> F [label=Y]
	H -> F [label=Z]
	I -> J [label="#"]
	J -> K [label="#"]
	K -> L [label=0]
	K -> M [label=1]
	K -> N [label=X]
	L -> K [label=Y]
	M -> K [label=Z]
	N -> O [label=0]
	N -> P [label=1]
	N -> Q [label=X]
	O -> N [label=Y]
	P -> N [label=Z]
	Q -> accept [label="#"]
}
