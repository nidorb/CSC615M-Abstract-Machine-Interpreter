digraph {
	A [label=R fillcolor=green shape=circle style=filled]
	B [label=R fillcolor=yellow shape=circle style=filled]
	C [label=R fillcolor=yellow shape=circle style=filled]
	D [label=L fillcolor=yellow shape=circle style=filled]
	E [label=R fillcolor=yellow shape=circle style=filled]
	F [label=R fillcolor=yellow shape=circle style=filled]
	G [label=R fillcolor=yellow shape=circle style=filled]
	H [label=L fillcolor=yellow shape=circle style=filled]
	I [label=R fillcolor=yellow shape=circle style=filled]
	J [label=L fillcolor=yellow shape=circle style=filled]
	K [label=L fillcolor=yellow shape=circle style=filled]
	A -> Z [label=a]
	A -> C [label=b]
	B -> A [label="#/X"]
	C -> D [label="#"]
	D -> E [label="X/#"]
	E -> F [label="#/X"]
	F -> E [label=b]
	F -> G [label=c]
	G -> H [label="#"]
	H -> I [label="X/#"]
	I -> H [label=c]
	I -> J [label="#"]
	J -> K [label="#"]
	K -> accept [label="#"]
}
