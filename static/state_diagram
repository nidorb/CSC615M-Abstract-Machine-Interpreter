digraph {
	A [label=S fillcolor=green shape=circle style=filled]
	B [label=W fillcolor=yellow shape=circle style=filled]
	C [label=R fillcolor=yellow shape=circle style=filled]
	D [label=W fillcolor=yellow shape=circle style=filled]
	E [label=S fillcolor=yellow shape=circle style=filled]
	F [label=W fillcolor=yellow shape=circle style=filled]
	G [label=R fillcolor=yellow shape=circle style=filled]
	H [label=S fillcolor=yellow shape=circle style=filled]
	I [label=R fillcolor=yellow shape=circle style=filled]
	A -> B [label=a]
	A -> D [label=a]
	A -> C [label=b]
	B -> A [label=X]
	B -> C [label=X]
	C -> D [label=X]
	D -> E [label=Y]
	E -> C [label=b]
	E -> F [label=c]
	F -> G [label="#"]
	G -> H [label=Y]
	H -> G [label=c]
	H -> I [label="#"]
	I -> accept [label="#"]
}
